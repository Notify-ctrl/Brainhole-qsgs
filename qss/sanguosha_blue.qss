}

QComboBox:!editable:on {
    background-color: none; /* GundamKillV2 */
    color: white;
}

QComboBox {
    border: 2px solid rgb(185, 195, 199);
    border-radius: 5px;
    height: 30px;
}

QComboBox:editable:focus, QComboBox:editable:on {
    border-color: rgb(0, 131, 253); /* GundamKillV2 */
}

QComboBox::drop-down {
    background: rgb(236, 240, 255); /* GundamKillV2 */
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
    min-width: 30px;
}

QComboBox::drop-down:hover {
    background-color: rgb(53, 158, 255); /* GundamKillV2 */
}

QComboBox::drop-down:on {
    background-color: rgb(38, 66, 194); /* GundamKillV2 */
}

QComboBox::drop-down:disabled {
    background-color: rgba(213, 219, 255, 70%); /* GundamKillV2 */
}

QComboBox::down-arrow {
    image: url(image/system/controls/combobox-down-arrow.png);
}

QComboBox QAbstractItemView {
    background: rgb(52, 73, 94);
