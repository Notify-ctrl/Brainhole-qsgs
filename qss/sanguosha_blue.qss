QGroupBox {
    border-radius: 5px;
    background: transparent;
    background-color: rgba(215, 235, 255, 0%); /* GundamKillV2 */
    border: 2px solid rgb(185, 195, 199);
    margin-top: 7px;
    padding: 1px; /* GundamKillV2 */
}

QGroupBox::title {
    padding: 0 3px;
    subcontrol-origin: margin;
    subcontrol-position: top center;
}

QGroupBox:disabled {
    background-color: rgba(215, 235, 255, 0%); /* GundamKillV2 */
    border-color: rgba(213, 219, 255, 70%); /* GundamKillV2 */
