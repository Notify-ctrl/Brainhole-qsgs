}

QLineEdit {
    border: 2px solid rgb(185, 195, 199);
    padding-left: 8px;
    min-height: 20px; /* GundamKillV2 */
    border-radius: 6px;
    alignment: left center;
}

QLineEdit:focus {
    border-color: rgb(0, 131, 253); /* GundamKillV2 */
}

QLineEdit:disabled {
    background-color: rgba(215, 235, 255, 70%); /* GundamKillV2 */
    border-color: rgba(213, 219, 255, 70%); /* GundamKillV2 */
    color: rgba(163, 169, 169, 70%);
}

QDialog {
    background: rgb(214, 231, 255); /* GundamKillV2 */
}

QTabWidget::pane {
    border: none;
}

QTabWidget::tab-bar {
    subcontrol-position: center;
}

QTabBar::tab {
    alignment: center;
    background-color: rgb(236, 240, 255); /* GundamKillV2 */
    border: none;
    width: 109;
    height: 44;
    color: rgb(52, 73, 94);
    font: 13px;
}

QTabBar::tab:only-one, QTabBar::tab:first {
    border-top-left-radius: 5px;
    border-bottom-left-radius: 5px;
}

QTabBar::tab:only-one, QTabBar::tab:last {
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
}

QTabBar::tab:selected {
    background-color: rgb(0, 131, 253); /* GundamKillV2 */
}

QTabBar::tab:!selected:hover {
    color: rgb(0, 131, 253); /* GundamKillV2 */
}

QTabBar::scroller {
    width: 52px;
}

QTabBar QToolButton {
    background-color: rgb(236, 240, 255); /* GundamKillV2 */
    border: transparent;
    border-radius: 5px;
    alignment: center;
}

QTabBar QToolButton:hover {
    background-color: rgb(0, 131, 253); /* GundamKillV2 */
}

QTabBar QToolButton::left-arrow {
    image: url(image/system/controls/tabbar-left-arrow.png);
    width: 12;
    height: 12;
	background-color: rgb(0, 131, 253, 50%); /* GundamKillV2 */
	border-radius: 5px; /* GundamKillV2 */
}

QTabBar QToolButton::right-arrow {
    image: url(image/system/controls/tabbar-right-arrow.png);
    width: 12;
    height: 12;
	background-color: rgb(0, 131, 253, 50%); /* GundamKillV2 */
	border-radius: 5px; /* GundamKillV2 */
}

QMenuBar {
    background-color: rgb(236, 240, 255); /* GundamKillV2 */
}

QMenuBar::item {
    spacing: 5px; /* spacing between menu bar items */
    padding: 2px 4px;
    background: transparent;
    border-radius: 4px;
}

QMenuBar::item:selected { /* when selected using mouse or keyboard */
    color: rgb(0, 131, 253); /* GundamKillV2 */
}

QMenuBar::item:pressed {
    background: rgb(0, 131, 253); /* GundamKillV2 */
    color: rgb(255, 255, 255);
}


QMenu {
    background-color: rgb(214, 231, 253); /* GundamKillV2 */
    border: none;
}

QMenu::item {
    padding: 7px 40px 7px 35px;
    background: transparent;
    border-radius: 2px;
}

QMenu::item:selected {
    background: rgba(88, 201, 241, 200);
}

QMenu::separator {
    height: 1px;
    background: qlineargradient(spread:reflect, x1:0, y1:0, x2:0.5, y2:0, stop:0 transparent, stop:1 rgba(72, 160, 212, 255));
    border-radius: 1px;
    margin-left: 5px;
    margin-right: 5px;
}

QMenu::indicator:checked {
    margin-left: 5px;
    width: 15px;
    height: 15px;
    image: url(image/system/controls/menu-indicator.png);
}

QMenu::right-arrow {
    margin-right: 5px;
    width: 12px;
    height: 12px;
    image: url(image/system/controls/menu-right-arrow.png);
}

QSpinBox {
    border: 2px solid rgb(185, 195, 199);
    padding-left: 8px;
    min-height: 15px; /* GundamKillV2 */
    border-radius: 5px;
    alignment: left center;
}

QSpinBox:focus {
    border-color: rgb(0, 131, 253); /* GundamKillV2 */
}

QSpinBox:disabled {
    background-color: rgba(215, 235, 255, 70%); /* GundamKillV2 */
    border-color: rgba(213, 219, 255, 70%); /* GundamKillV2 */
    color: rgba(163, 169, 169, 70%);
}

QSpinBox::up-button {
    margin-top: 1px;
    margin-right: 1px;
    background: rgb(236, 240, 255); /* GundamKillV2 */
    border-top-right-radius: 3px;
}

QSpinBox::down-button {
    margin-bottom: 1px;
    margin-right: 1px;
    background: rgb(236, 240, 255); /* GundamKillV2 */
    border-bottom-right-radius: 3px;
}

QSpinBox::up-button:hover, QSpinBox::down-button:hover {
    background: rgb(0, 131, 253); /* GundamKillV2 */
}

QSpinBox::up-button:disabled, QSpinBox::down-button:disabled {
    background-color: rgba(213, 219, 255, 70%); /* GundamKillV2 */
}

QSpinBox::up-arrow {
    image: url(image/system/controls/spinbox-up-arrow.png);
}

QSpinBox::down-arrow {
    image: url(image/system/controls/spinbox-down-arrow.png);
}

QCheckBox {
    spacing: 10px;
    margin-left: 10px;
}

QCheckBox:checked {
    color: rgb(0, 131, 253); /* GundamKillV2 */
}

QCheckBox::indicator {
    background: rgb(189, 195, 199);
    border-radius: 5px;
    height: 20px;
    width: 20px;
}

QCheckBox::indicator:checked {
    background: rgb(0, 131, 253); /* GundamKillV2 */
    image: url(image/system/controls/checkbox-indicator.png);
}

QCheckBox::indicator:hover {
    image: url(image/system/controls/checkbox-indicator.png);
}

QCheckBox:disabled {
    color: rgb(200, 202, 204);
}

QCheckBox::indicator:disabled {
    background: rgb(200, 202, 204);
}

QDialog QPushButton {
    color: white;
    background-color: rgb(0, 131, 253); /* GundamKillV2 */
    border: transparent;
    border-radius: 5px;
    min-height: 25px; /* GundamKillV2 */
    min-width: 75px; /* GundamKillV2 */
}

QDialog QPushButton:hover {
    background-color: rgb(53, 158, 255); /* GundamKillV2 */
}

QDialog QPushButton:disabled {
    background-color: rgba(53, 158, 255, 52%); /* GundamKillV2 */
